<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="$(SolutionDir)\ReleaseNotes.csproj.import" />
  <PropertyGroup>
    <PackageId>WebsocketSharp.Core</PackageId>
    <RootNamespace>WebsocketSharp.Core</RootNamespace>
    <AssemblyName>WebsocketSharp.Core</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2022.0.02.23</Version>
    <Product>WebsocketSharp.Core - efae8e82</Product>
    <Authors>sta.blockhead</Authors>
    <Description>A C# implementation of the WebSocket protocol client and server</Description>
    <Copyright>Copyright Â© sta.blockhead 2010-2022</Copyright>
    <PackageProjectUrl>https://github.com/sta/websocket-sharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sta/websocket-sharp</RepositoryUrl>
    <RespositoryType>git</RespositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>WebSocket;.NETCore;</PackageTags>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>websocket-sharp.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="AssemblyInfo.cs" />
  </ItemGroup>

</Project>
